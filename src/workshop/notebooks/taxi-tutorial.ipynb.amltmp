        "\n",
        "taxi_holidays_df = pd.merge(green_taxi_df, holidays_df, how=\"left\", on=[\"datetime\", \"country_code\"])\n",
        "taxi_holidays_df[taxi_holidays_df[\"normalizeHolidayName\"].notnull()]"
